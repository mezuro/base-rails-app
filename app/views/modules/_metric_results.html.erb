<table class="table table-hover">
  <thead>
    <th> <%= t('metric') %> </th>
    <th> <%= t('value') %></th>
    <th> <%= t('weight') %></th>
    <th> <%= t('threshold') %></th>
  </thead>

  <tbody>
    <% cache("#{@root_module_result.id}_results") do %>
      <%= render partial: 'metric_result', collection: @root_module_result.metric_results, locals: {module_result: @root_module_result} %>
    <% end %>
  </tbody>
</table>